{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "all",
  "endOfLine": "lf",
  "bracketSpacing": false,
  "arrowParens": "always",
  "proseWrap": "always",
  "printWidth": 92,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "printWidth": 78
      }
    }
  ]
}
